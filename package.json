{
  "name": "mad-science-stack",
  "version": "0.0.0",
  "description": "bleeding magic stack example",
  "main": "server",
  "scripts": {
    "postinstall": "npm run modules",
    "modules": "linklocal link -r && linklocal list -r | bulk -c 'npm install'",
    "lint": "snazzy",
    "format": "snazzy --format",
    "test": "npm-run-all -p test:*",
    "test:spec": "node spec",
    "test:feature": "node features",
    "dev:client": "watchify client -o build/bundle.js -dv -p browserify-hmr",
    "dev:assets": "cpx \"assets/**/*\" build -w",
    "dev:api": "node-dev ./api/cli",
    "dev:static": "node-dev ./static/cli --livereload",
    "dev:livereload": "wtch -d build",
    "prod:client": "browserify client -o build/bundle.js -g envify -g uglifyify",
    "prod:assets": "cpx \"assets/**/*\" build",
    "prod:api": "node ./api/cli",
    "prod:static": "node ./static/cli",
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "prod": "NODE_ENV=production npm-run-all -s prod:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspiral-craftworks/mad-science-stack.git"
  },
  "keywords": [
    "stack",
    "example",
    "react",
    "redux",
    "frp",
    "functional",
    "reactive",
    "immutable",
    "feathers",
    "ddd",
    "tcomb"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enspiral-craftworks/mad-science-stack/issues"
  },
  "homepage": "https://github.com/enspiral-craftworks/mad-science-stack#readme",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.8.0",
    "browserify-hmr": "^0.3.1",
    "cuke-tap": "^1.0.2",
    "ecstatic-lr": "^1.0.1",
    "glob": "^6.0.2",
    "jsdom": "^7.1.0",
    "node-dev": "^2.7.1",
    "redux-logger": "^2.0.4",
    "run-parallel": "^1.1.4",
    "tape": "^4.4.0",
    "watchify": "^3.6.1",
    "wtch": "^4.0.1"
  },
  "dependencies": {
    "api": "file:./api",
    "app": "file:./app",
    "babel-core": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.2.0",
    "babelify": "^7.2.0",
    "browserify": "github:ahdinosaur/node-browserify",
    "bulk": "^2.0.0",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "client": "file:./client",
    "config": "file:./config",
    "cpx": "^1.2.1",
    "cssify": "github:ahdinosaur/cssify",
    "envify": "^3.4.0",
    "linklocal": "^2.5.2",
    "npm-run-all": "^1.3.2",
    "pinkie-promise": "^2.0.0",
    "ramda": "^0.18.0",
    "sheetify": "^3.3.3",
    "static": "file:./static",
    "uglifyify": "^3.0.1"
  }
}
